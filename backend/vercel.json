{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "/server.js",
      "methods": ["GET", "HEAD", "POST", "PUT", "PATCH", "DELETE"],
      "headers": { "Access-Control-Allow-Origin": "*" }
    }
  ]
}
